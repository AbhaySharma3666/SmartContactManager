# Application Configuration
BASE_URL=http://localhost:8080

# Aiven MySQL Database Configuration
AIVEN_HOST=your-mysql-service.aivencloud.com:port
AIVEN_DB=defaultdb
AIVEN_USER=avnadmin
AIVEN_PASSWORD=your-aiven-password

# Optional Features
# email configuration
EMAIL_USERNAME=your-email@gmail.com
EMAIL_PASSWORD=your-app-password

# google configureation
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# github configureation
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret

# Cloudinary Configuration
CLOUDINARY_CLOUD_NAME=your-cloud-name
CLOUDINARY_API_KEY=your-api-key
CLOUDINARY_API_SECRET=your-api-secret

# 2Factor SMS Configuration
TWOFACTOR_API_KEY=your-2factor-api-key
